{
  "$schema": "https://json.schemastore.org/tsconfig.json",
  "extends": "../tsconfig.base.json",
  "include": ["**/*"],
  "exclude": ["**/*.test.ts", "../node_modules/**", "test-utils/**"],
  "compilerOptions": {
    "exactOptionalPropertyTypes": false,
    "noEmit": false,
    "module": "ES2022",
    "moduleResolution": "Node",
    "outDir": "../pre",
    "declaration": true,
    "declarationDir": "../dist"
  }
}
